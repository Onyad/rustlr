# grammar for propositional logic

!use fixedstr::str4;

nonterminal Prop
terminal OR AND IMPLIES NOT ( )
typedterminal Var str4
topsym Prop

lexname OR |
lexname AND &
lexname NOT ~
lexname IMPLIES ->
lexvalue Var Alphanum(x) str4::from(x)

left OR 50
left AND 80
left NOT 100
left IMPLIES 30

Prop:Var --> Var
Prop:And --> Prop AND Prop
Prop:Or --> Prop OR Prop
Prop:Implies --> Prop IMPLIES Prop
Prop:Not --> NOT Prop
Prop --> ( Prop )
#Prop --> ( Prop:p )  { p }
EOF
